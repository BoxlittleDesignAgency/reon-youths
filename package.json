{
  "name": "reonyouths",
  "version": "1.0.0",
  "description": "Reon Youths Evangelistic Ministry",
  "main": "index.js",
  "engines": {
    "node": "13.11.0",
    "npm": "6.14.5",
    "yarn": "1.22.4"
  },
  "author": "Demola Adebayo",
  "scripts": {
    "start": "nodemon server.js",
    "client-install": "cd client && yarn install",
    "install-client": "cd client && yarn install",
    "client": "cd client && yarn start",
    "build": "cd client && yarn run build",
    "dev": "concurrently \"yarn run start\" \"yarn run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false  npm run install-client && npm run build"
  },
  "keywords": [
    "node",
    "api"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "normalize-url": "^5.0.0",
    "path": "^0.12.7",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "crypto": "^1.0.1",
    "express-jwt": "^5.3.3",
    "formidable": "^1.2.2",
    "validator": "^13.0.0"
  }
}
